# optimizes geartrain with ea

environment
  tabular_data
    tabular_data_file = 'results_ea1.dat'

method
  coliny_ea max_function_evaluations = 1000 # EA
    seed  =  1

variables # adjust number of variables
  continuous_design = 4
    lower_bounds = 4*12
    upper_bounds = 4*60

interface
  fork
    analysis_driver = 'python geartrain_driver.py'
 

responses
  num_objective_functions = 2
  no_gradients
  no_hessians
