# optimizes zdt1 with ea

environment
  tabular_data
    tabular_data_file = 'results_ea1.dat'

method
  coliny_ea max_function_evaluations   = 250   #COLINY_EA
    seed  =  1

variables # adjust number of variables
  continuous_design = 30
    lower_bounds = 30*0
    upper_bounds = 30*1

interface
  fork
    analysis_driver = 'python zdt1_driver.py'
 

responses
  num_objective_functions = 2
  no_gradients
  no_hessians
