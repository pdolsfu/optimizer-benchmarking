# This .in file optimizes the schubert10 problem with ea

environment
    tabular_data
tabular_data_file  =  'results_{method}10.dat'

method 
  coliny_ea             population_size  =  50 max_function_evaluations = 1000   # ea (coliny)
seed  =  10
	
model
   single

variables  # modify variables if needed
  continuous_design = 10
    upper_bounds     10*5.12
    lower_bounds     10*-5.12

interface
   analysis_drivers = 'python schubert_driver.py' #SCHUBERT
   fork
        parameters_file = 'params.in'
        results_file    = 'results.out'

responses
   objective_functions = 1
   no_gradients
   no_hessians