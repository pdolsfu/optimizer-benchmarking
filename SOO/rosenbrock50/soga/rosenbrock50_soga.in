# This .in file optimizes the rosenbrock50 problem with soga

environment
    tabular_data
tabular_data_file  =  'results_soga10.dat'

method 
  soga                  max_iterations = 20 population_size = 50 		 # SOGA
seed  =  10
	
model
   single

variables  # modify variables if needed
  continuous_design = 50
    upper_bounds     50*10
    lower_bounds     50*-10

interface
   analysis_drivers = 'python rosenbrock_driver.py' #ROSEN
   fork
        parameters_file = 'params.in'
        results_file    = 'results.out'

responses
   objective_functions = 1
   no_gradients
   no_hessians