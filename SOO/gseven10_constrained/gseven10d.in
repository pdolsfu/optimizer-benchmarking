# TEMPLATE for driver file, optimizes the gseven10d problem with {method} (500 max function evaluations)

environment
    tabular_data
	tabular_data_file  =  'results_{method}1.dat'   # {method} to be swapped out in each of the method folders

method 
  soga                  max_iterations = 10 population_size = 50 		 # SOGA
  coliny_direct         max_function_evaluations = 500          		 # DIRECT
  coliny_ea             population_size  =  50 max_function_evaluations = 5000   # EA
  efficient_global 	max_iterations = 400 initial_samples = 100               # EGO
  
  ncsu_direct           max_function_evaluations = 500
  genie_direct          max_function_evaluations = 500
  genie_opt_darts       max_function_evaluations = 500
     seed  =  1
	

model
   single

variables  # modify variables if needed
  continuous_design = 10
    upper_bounds     10*10
    lower_bounds     10*-10

interface
   analysis_drivers = 'python gseven_driver.py'
   fork
        parameters_file = 'params.in'
        results_file    = 'results.out'

responses
   objective_functions = 1
   nonlinear_inequality_constraints = 8 # add nonlinear constraints if needed
   no_gradients
   no_hessians