# This .in file optimizes the michalewicz30 problem with GENIE

environment
    tabular_data
tabular_data_file  =  'results_GENIE10.dat'

method 
  genie_direct          max_function_evaluations = 1000                          # GENIE
seed  =  10
	
model
   single

variables  # modify variables if needed
  continuous_design = 30
    upper_bounds     30*3.141593
    lower_bounds     30*0

interface
   analysis_drivers = 'python michalewicz_driver.py' #MICHALEWICZ
   fork
        parameters_file = 'params.in'
        results_file    = 'results.out'

responses
   objective_functions = 1
   no_gradients
   no_hessians