# optimizes zdt6 with moga

environment
  tabular_data
    tabular_data_file = 'results_moga10.dat'

method
  moga max_function_evaluations = 250 # MOGA
    seed  =  10

variables # adjust number of variables
  continuous_design = 10
    lower_bounds = 10*0
    upper_bounds = 10*1

interface
  fork
    analysis_driver = 'python zdt6_driver.py'
 

responses
  num_objective_functions = 2
  no_gradients
  no_hessians
